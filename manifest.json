{
	"name": "Qrome",
	"version": "0.1.0",
	"manifest_version": 2,
	"description": "Chrome OS QQ客户端",
	"minimum_chrome_version": "25.0.1364.97",
	"browser_action": {
		"default_icon": {
			"19": "images/logo.png",
			"38": "images/logo.png"
		},
		"default_title": "Qrome"
	},
	"icons": {
		"16": "images/logo.png",
		"48": "images/logo.png",
		"128": "images/logo.png"
	},
	"background": {
		"page": "background.html"
	},
	"options_page": "settings.html",
	"permissions": [
		"background",
		"notifications",
		"cookies",
		"tabs",
		"storage",
		"unlimitedStorage",
		"webRequest",
		"webRequestBlocking",
		"fontSettings",
		"http://*/*",
		"https://*/*"
	],
	"web_accessible_resources": [
		"images/logo.png",
		"images/bot/*"
	]
}